<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.isacs.sosmecanica">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_sosmecanica"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name=".CategoriaActivity" />
        <activity android:name=".PostsActivity" />
        <activity android:name=".GritarActivity" />
        <activity android:name=".SocorrerActivity" />
        <activity android:name=".PoliticaActivity"></activity>
    </application>

</manifest>